load("//shared/bazel/rules:cc_rules.bzl", "wpilib_cc_library")

wpilib_cc_library(
    name = "protobuf-headers",
    hdrs = glob([
        "include/**/*.h",
        "include/**/*.inc",
    ]),
    includes = ["include"],
    strip_include_prefix = "include",
)

filegroup(
    name = "protobuf-srcs",
    srcs = glob(["src/**"]),
)
